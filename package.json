{
  "name": "itsvinayak",
  "version": "0.1.0",
  "private": true,
  "description": "My personal website",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "find:unused": "next-unused",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "tailwind:build": "postcss styles/index.css -o public/build/tailwind.css",
    "tailwind:watch": "postcss styles/index.css -o public/build/tailwind.css --watch",
    "tailwind:prod": "DEPLOYMENT_TYPE=production npm run tailwind:build"
  },
  "author": {
    "name": "Vinayak",
    "email": "itsvinayak@gmail.com"
  },
  "next-unused": {
    "exclude": [
      "node_modules",
      ".next",
      "styles",
      "types",
      "public",
      "content"
    ],
    "entrypoints": [
      "src/app",
      "src/components"
    ]
  },
  "license": "MIT",
  "displayName": "itsvinayak",
  "keywords": [
    "next",
    "next.js",
    "react",
    "tailwind",
    "tailwindcss",
    "typescript",
    "Vinayak",
    "Vinayak Sharma",
    "itsvinayak"
  ],
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/mdx": "^2.0.10",
    "@vercel/speed-insights": "^1.0.2",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "next": "^14.0.4",
    "next-contentlayer": "^0.3.4",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.11.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.0",
    "shiki": "^0.14.5",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "next-unused": "^0.0.6",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "typescript": "^5"
  }
}
